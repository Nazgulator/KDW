            var BTNStorno = $('#ButtonsStorno');
            var Buttons = $('#Buttons');
         TRHIDE();
          var url2 = "@Url.Action("FindLastStatusOTKJSON", "POOrderEntries")?QRData=" + Num ;
        $.post
            (url2, function (data)
            {
                var S = data.split(';');
               
                if (S[0] =='TRUE')
                {
                    $("#StatusOTK").addClass('btn-success');
                    $("#StatusOTK").removeClass('btn-danger');
                    $("#StatusOTK").val(S[1]);
                    QTYVQR();
                    QTYVNZP();
                    QTYNeedToday();
                    QTYNeedTomorrow();
                    QTYCINToday();
                    QTYINStock();
                    FindQTYPeremestit();
                    $("#StatusOTK").val(S[1]);
                }
                else
                {
                    $("#StatusOTK").addClass('btn-danger');
                    $("#StatusOTK").removeClass('btn-success');
                    $("#StatusOTK").val(S[1]);
                   // $("#StatusOTK").html('Можно нельзя переместить в НЗП');
                }
                if ($('#StornoCHK').is(":checked")) {
                    BTNStorno.show('slow');
                }
                // alert(Storno);
                Peremestit.show('slow');
                if (!Storno) {

                    Buttons.show('slow');
                    BTNStorno.hide('slow');
                }
                else
                {
                    Buttons.hide('slow');
                    BTNStorno.show('slow');
                }
                TR.show('slow');
            }

        );
    }
 
</script>

