          var QR = $("#NPartNumber").val();

        var url2 = "@Url.Action("AutocompleteSearchQTYByQR", "POOrderEntries")?QRData=" + QR ;
        $.post
            (url2, function (data)
            {
                var S = data.split(';');
                FNumber.val(S[0]);
                FQtyFact.val(S[1]);
                FQtyMax.val(S[2]);
                Stock.val(S[3]);
             //   alert(data);
                // $("#SkolkoPeremestit").val(data);
