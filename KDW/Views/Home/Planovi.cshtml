

<!-- Modal -->
<div class="modal fade" id="Download" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header">




                <h4>@Resources.Resource.ZagruzkaDannih...</h4>
                <button id="X" type="button" class="close hide" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="progress">

                    <div id="progressBar" class="progress-bar progress-bar-striped bg-success progress-bar-animated" role="progressbar" style="width: 0%" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100"></div>
                    <center>
                        <label id="nadpis">@Resources.Resource.ZagruzkaDannih...</label>
                    </center>
                </div>
            </div>
        </div>
    </div>
</div>




<div class="container">

    <div class="table table-striped">
        <div class="row">
            <div class="col">
                <h5>              </h5>
            </div>
        </div>
        <div class="row">
            <div class="col" style="text-align:center">
                <h1>@Resources.Resource.Planovi</h1>
            </div>
        </div>


        <div class="row">
            <div class="col" style="text-align:center">
                <center>


                    <p>
                        <button type="button" class="btn btn-block btn-success glyphicon glyphicon-alert" style="width:500Px" data-toggle="modal" data-target="#Download" onclick="location.href='@Url.Action("StatusWork", "POOrderEntries")'">@Resources.Resource.Status</button>

                    </p>
                    <p>
                        <button type="button" class="btn btn-block btn-success glyphicon glyphicon-adjust" style="width:500Px" data-toggle="modal" data-target="#Download" onclick="location.href='@Url.Action("UploadPlanovi", "POOrderEntries")'">@Resources.Resource.LoadFileVipuska</button>

                    </p>
                    <p>
                        <button type="button" class="btn btn-block btn-success glyphicon glyphicon-align-left" style="width:500Px" data-toggle="modal" data-target="#Download" onclick="location.href='@Url.Action("PlanoviWorks", "POOrderEntries")'">@Resources.Resource.Plan3Days</button>

                    </p>
                    <p>
                        <button type="button" class="btn btn-block btn-success glyphicon glyphicon-time" style="width:500Px" data-toggle="modal" data-target="#Download" onclick="location.href='@Url.Action("PerenosDatWork", "POOrderEntries")'">@Resources.Resource.ObnovleniePlanaRabot</button>
                    </p>
                </center>
            </div>
        </div>



    </div>

</div>

@Html.Action("UserNow", "POOrderEntries")






