                            <p>
                                <button type="button" class="btn btn-block btn-warning glyphicon glyphicon-scissors" style="width:500Px" data-toggle="modal" data-target="#Download" onclick="location.href='@Url.Action("ScanGP", "GPScans")'">@Resources.Resource.ScanGP</button>
                            </p>

                        }
                        @if (ViewBag.Roles.Contains("Мастер"))
